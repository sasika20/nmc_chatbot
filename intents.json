{
  "intents": [
    {
      "tag": "greeting",
      "patterns": ["Hi", "Hello", "Good day", "Is anyone there?", "Hey", "Greetings"],
      "responses": ["Hello! Welcome to NMC Chatbot. How can I assist you today?", "Hi there! Ask me about NMC, Puthanampatti.", "Good to see you! What would you like to know about the college?"]
    },
    {
      "tag": "contact",
      "patterns": ["How can I contact the college?", "College phone number", "NMC email address"],
      "responses": ["You can reach the college at **+91 98429 56720** or email at **nmcfrontoffice@gmail.com**. The campus is located in Puthanampatti, Tiruchirappalli - 621007."]
    },
    {
      "tag": "thanks",
      "patterns": ["Thank you", "Thanks a lot", "That helps", "Good job"],
      "responses": ["You're welcome! Feel free to ask if you have any other questions.", "Happy to help! Have a great day.", "Glad I could assist you."]
    },
    { 
      "tag": "bye",
      "patterns": ["Bye", "Goodbye", "See you later", "Gotta go"],
      "responses": ["Goodbye! Visit our website at www.nmc.ac.in for more details.", "Have a productive day!", "See you soon!"]
    },
    
    
    {
      "tag": "college_name_query",
      "patterns": ["What is the college name?", "NMC full name", "What is NMC full form", "Where is the college located"],
      "responses": ["GET_COLLEGE_NAME_AND_LOCATION"]
    },
    {
      "tag": "principal_query",
      "patterns": ["who is the principal", "principal name", "who is the head", "name of the principal", "secretary name", "who is the vice principal"],
      "responses": ["GET_ADMIN_PERSONNEL"]
    },
    {
      "tag": "facilities_query",
      "patterns": ["what facilities are available", "college facilities", "infrastructure details", "Wi-Fi speed", "total labs"],
      "responses": ["GET_FACILITIES_SUMMARY"]
