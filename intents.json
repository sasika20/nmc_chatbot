    {
      "tag": "hostel_query",
      "patterns": ["hostel facility", "boys hostel capacity", "girls hostel capacity", "hostel details", "hostel names"],
      "responses": ["GET_HOSTEL_DETAILS"]
    },
    {
      "tag": "programs_count_query",
      "patterns": ["How many courses are offered", "total number of programs", "total courses", "how many UG and PG programs"],
      "responses": ["GET_PROGRAMS_COUNT"]
    },
    {
      "tag": "pg_query",
      "patterns": ["PG courses available", "Masters programs list", "MSc courses", "MBA and MCA programs"],
      "responses": ["GET_PG_COURSES"]
    },
    {
      "tag": "placement_query",
      "patterns": ["placement details", "highest salary", "average salary", "top recruiters", "placement statistics"],
      "responses": ["GET_PLACEMENT_SUMMARY"]
    },
    {
      "tag": "accreditation_query",
      "patterns": ["NAAC grade", "NAAC accreditation status", "What is the NAAC score", "NAAC status"],
      "responses": ["GET_NAAC_ACCREDITATION"]
    },
    {
      "tag": "data_science_query",
      "patterns": ["data science courses", "MSc Data Science", "BSc Data Science", "who is the HOD for data science", "details about data science"],
      "responses": ["GET_DATA_SCIENCE_DETAILS"]
    },
    {
